generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

enum Gender {
  MALE
  FEMALE
  OTHER
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String?  @unique
  gender    Gender
  dob       DateTime
  createdAt DateTime @default(now())
}
